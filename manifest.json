{
   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "豆瓣刷刷"
   },
   "content_scripts": [ {
      "js": [ "jquery.min.js", "background.js" ],
      "matches": [ "https://www.douban.com/*"],
